#!/bin/bash
#SBATCH --job-name=within_subject_adjusted_questionnaire
#SBATCH --output=/oak/stanford/groups/russpold/data/uh2/aim1/analysis_code/survey_medley_code/log/adj_question_maps_%A_%a.out
#SBATCH --error=/oak/stanford/groups/russpold/data/uh2/aim1/analysis_code/survey_medley_code/log/adj_question_maps_%A_%a.err
#SBATCH --time=01:00:00
#SBATCH -p russpold,hns,normal
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=4
#SBATCH --mem=16G
#SBATCH --array=0-96%20

PROJECT_ROOT="/oak/stanford/groups/russpold/data/uh2/aim1/analysis_code/survey_medley_code"
SCRIPT_DIR="$PROJECT_ROOT/analyses/within_subject_adjusted_questionnaire_averages"
UV_SETUP="$PROJECT_ROOT/setup_uv_sherlock.sh"

source $UV_SETUP

uv --directory "$PROJECT_ROOT" run "$SCRIPT_DIR/run_model.py" "$SLURM_ARRAY_TASK_ID" 